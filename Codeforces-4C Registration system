#include <bits/stdc++.h>
using namespace std;
int main()
{
    long long int i,n,t;
    cin>>n;
    string s;
    map<string, long long int> m;
    while(n--){
    cin>>s;
    
    if(m.count(s))
    {
        m[s]++;
        cout<<s<<m[s]-1<<endl;
        continue;
    }
    
    cout<<"OK"<<endl;
    m[s]=1;
    
    }
	return 0;
}
